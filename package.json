		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-memo": "^0.0.3",
		"kanel": "^3.5.5",
		"next-translate-plugin": "^2.6.2",
		"pre-commit": "^1.2.2",
		"prettier": "^3.1.0",
		"tsx": "^4.1.3"
	},
	"pre-commit": [
		"typecheck",
		"prettier",
		"lint"
